
@{
    ViewBag.Title = "XYZ Inc. Staff";
    string[] staff = { "Fred Flintstone", "Barney Rubble", "Judy Jetson", "Pebbles Flintstone"};
    int staffLen = staff.Length;
    string[] managers = { "Fred Jones", "Shaggy Rogers", "Wilma Flintstone"};
    int managerLen = managers.Length;
    string[] officers = { "Astro Jetson", "Scooby Doo", "Daphne Blake"};
    int officersLen = officers.Length;
    var x = 0;
}
<hr />
<img src="~/Content/detectives.jpg" alt="XYZ Detectives" style="float:right; width:200px; outline: 5px solid black; outline-offset: 10px;" />
<h2>XYZ Dectective Agency Staff</h2>
@for (x = 0; x < staffLen; x++) {<p>@staff[x]</p> }
<hr />
<h2>XYZ Dectective Agency Managers</h2>
@foreach (var person in managers) 
{ 
    <p>@person</p>
}
<hr />
<h2>XYZ Dectective Agency Corprate Office</h2>
@{ 
    x = 0;
    while (x < officersLen)
    {
        <p>@officers[x]</p>
        x += 1;
    }
}
<hr />
